{
	"name": "Natural Dark",
	"colors": {
		"titleBar.activeBackground": "#151617",
		"foreground": "#eeffff",
		"focusBorder": "#05b48e",
		"selection.background": "#686868",
		"icon.foreground": "#6ff7af",

		"textLink.foreground": "#05b48e",
		"textLink.activeForeground": "#05b48e",
		
		"editor.background": "#121414",
		"editor.foreground": "#eeffff",
		"editor.selectionBackground": "#2f2f2f",
		"editorWidget.background": "#1c1c1c",
		"editorGroupHeader.tabsBackground": "#151617",
		"tab.activeBackground": "#292929",
		"tab.inactiveBackground": "#1a1a1a",

		"sideBar.background": "#151617",
		"sideBar.foreground": "#d9d9d9",
		"sideBarSectionHeader.background": "#1a1b1c",

		"statusBar.background": "#008060",
		"statusBar.debuggingBackground": "#003d27",
		"statusBarItem.remoteBackground": "#005c49",

		"activityBar.background": "#151617",
		"activityBar.activeBackground": "#007a60",
		"activityBarBadge.background": "#373737",

		"button.background": "#007a60",
		"checkbox.background": "#007a60",

		"list.activeSelectionBackground": "#2d2d2d",
		"list.inactiveSelectionBackground": "#2c2c2c",
		"list.highlightForeground": "#00ffbf",

		"commandCenter.background": "#232424",

		"settings.settingsHeaderHoverForeground": "#ccd9d2"
	},
	"tokenColors": [
		// Programming languages
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"variable.language"
			],
			"settings": {
				"foreground": "#87ffb9"
			}
		},
		{
			"name": "Control keywords",
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#a9e5b3",
				"fontStyle": ""
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.other",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#8effac"
			}
		},
		{
			"name": "Variable constants",
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#8effac",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable properties",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#c3ffc3"
			}
		},
		{
			"name": "Language constants",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#85fffd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#42fff6"
			}
		},
		{
			"name": "Preprocessor functions",
			"scope": [
				"entity.name.function.preprocessor",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"name": "Class types",
			"scope": [
				"entity.name.type",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#45f6c1",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Struct types",
			"scope": [
				"entity.name.type.struct"
			],
			"settings": {
				"foreground": "#aeff9c",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Interface types",
			"scope": [
				"entity.name.type.interface"
			],
			"settings": {
				"foreground": "#35ffb2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Enum types",
			"scope": [
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#35ffb2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Namespaces/module paths",
			"scope": [
				"entity.name.namespace",
				"entity.name.type.namespace"
			],
			"settings": {
				"foreground": "#d3d3d3",
				"fontStyle": ""
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator",
				"storage.modifier.pointer",
				"storage.modifier.reference"
			],
			"settings": {
				"foreground": "#dfdfdf",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String literal",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#f9d185"
			}
		},
		{
			"name": "String interpolation definition",
			"scope": [
				"punctuation.definition.interpolation",
				"punctuation.definition.template-expression",
			],
			"settings": {
				"foreground": "#dfeeee"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"meta.interpolation"
			],
			"settings": {
				"foreground": "#70ffc8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numerals",
			"scope": [
				"constant.numeric",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#ffea97"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#e8fff677",
				"fontStyle": ""
			}
		},

		// Markup
		{
			"name": "Markup paragraph",
			"scope": [
				"meta.paragraph"
			],
			"settings": {
				"foreground": "#eeffff"
			}
		},
		{
			"name": "Markup headings",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#4ccc9b",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#a2e9d9",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#a2e9d9",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup list",
			"scope": [
				"markup.list"
			],
			"settings": {
				"foreground": "#c0c0c0"
			}
		},
		{
			"name": "Markup quote",
			"scope": [
				"markup.quote",
			],
			"settings": {
				"foreground": "#ababab",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup code",
			"scope": [
				"markup.inline",
				"markup.fenced_code",
			],
			"settings": {
				"foreground": "#b2b2b2"
			}
		},
		{
			"name": "Markup link/image inline",
			"scope": [
				"meta.link.inline",
				"meta.image.inline",
			],
			"settings": {
				"foreground": "#d6d6d6",
			}
		},
		{
			"name": "Markup link title",
			"scope": [
				"string.other.link",
			],
			"settings": {
				"foreground": "#f9d185",
				"fontStyle": "bold underline"
			}
		},
		{
			"name": "Markup underline link",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#6fffd9",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup separator",
			"scope": [
				"meta.separator"
			],
			"settings": {
				"foreground": "#b0b0b0",
				"fontStyle": "bold"
			}
		},

		// XML, HTML
		{
			"name": "Entity tag",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#a2ffbe"
			}
		},
		{
			"name": "Entity attribute name",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#9efafd"
			}
		},
		{
			"name": "Entity tag punctuation",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#d5d5d5"
			}
		},

		// JSON
		{
			"name": "Support type",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#acffcf"
			}
		}
	],
	"semanticHighlighting": true
}